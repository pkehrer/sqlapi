{
  "name": "sqlapi",
  "author": "Paul Kehrer",
  "description": "package management for the sqlapi project (dotnet core)",
  "scripts": {
    "test": "dotnet test",
    "dotnet-publish": "dotnet publish -f netcoreapp2.2 -c Release",
    "docker-build": "docker build -t sqlapi .",
    "docker-runlocal": "docker run -p 8080:80 sqlapi:latest",
    "deploy-deploymentuser": "node aws/deploymentuser",
    "delete-deploymentuser": "node aws/deploymentuser delete",
    "deploy-ecr": "node aws/ecr",
    "delete-ecr": "node aws/ecr delete",
    "deploy-ecs": "node aws/ecs",
    "delete-ecs": "node aws/ecs delete",
    "deploy-db": "node aws/db",
    "delete-db": "node aws/db delete",
    "delete-all": "run-s delete-ecs delete-ecr delete-db delete-deploymentuser",
    "deploy-all": "run-s dotnet-publish docker-build deploy-deploymentuser deploy-ecr deploy-db deploy-ecs"
  },
  "dependencies": {
    "async-polling": "^0.2.1",
    "aws-sdk": "^2.384.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5"
  }
}
