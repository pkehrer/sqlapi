{
  "name": "sqlapi",
  "author": "Paul Kehrer",
  "description": "package management for the sqlapi project (dotnet core)",
  "scripts": {
    "test": "dotnet test",
    "dotnet-publish": "dotnet publish -f netcoreapp2.2 -c Release",
    "docker-build": "docker build -t sqlapi .",
    "docker-runlocal": "docker run -p 8080:80 sqlapi:latest",
    "deploy-aws": "node aws/deploy",
    "deploy": "run-s dotnet-publish docker-build deploy-aws",
    "delete-aws": "node aws/delete"
  },
  "dependencies": {
    "async-polling": "^0.2.1",
    "aws-sdk": "^2.384.0",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5"
  }
}
